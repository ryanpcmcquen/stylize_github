{
    "manifest_version": 2,

    "name": "Stylize GitHub",
    "description": "Tweaks the styling of https://github.com.",
    "version": "0.1.0",

    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": ["https://*.github.com/", "https://*.github.com/*"],
            "js": ["stylizeGitHub.js"]
        }
    ]
}
